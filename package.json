{
  "name": "stockland3d",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode=dev",
    "build": "tsc -b || true && vite build --mode=production",
    "build:dev": "tsc -b || true && vite build --mode=dev",
    "build:staging": "tsc -b || true && vite build --mode=staging",
    "build:prod": "tsc -b || true && vite build --mode=production",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "eslint": "eslint --fix src",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "eslint --fix src && prettier --write .",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --depthLimit 3 && prettier --write --log-level silent __generated__/**/*.gql",
    "codegen": "graphql-codegen --config codegen/typegen.yml && npm run schema:codegen",
    "typegen": "graphql-codegen --config codegen/typegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.9",
    "@mui/material": "^6.4.8",
    "@mui/x-data-grid": "^7.28.1",
    "apollo-link-timeout": "^5.0.0",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "firebase": "^11.5.0",
    "formik": "^2.4.6",
    "graphql-ws": "^6.0.4",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.0",
    "react-router-dom": "^7.4.0",
    "react-toastify": "^11.0.5",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "gql-generator-type": "^1.1.2",
    "graphql-tag": "^2.12.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
